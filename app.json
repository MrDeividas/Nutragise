{
  "expo": {
    "name": "Nutrapp",
    "slug": "nutrapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "statusBarStyle": "dark",
      "statusBarBackgroundColor": "#ffffff",
      "bundleIdentifier": "com.anonymous.nutrapp",
      "merchantIdentifier": "merchant.com.anonymous.nutrapp",
      "infoPlist": {
        "NSHealthShareUsageDescription": "We need access to your health data to automatically track your sleep, steps, and exercise habits.",
        "NSHealthUpdateUsageDescription": "We need permission to save your health data.",
        "UIBackgroundModes": ["fetch"]
      },
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.access": []
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.anonymous.nutrapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
